class EloquentController extends Controller
{
    public function index(){

        $post = Post::find(1);
        $image = $post->image;

        $activity = Activity_feed::find(1);
        $comment = $activity->comment;

        return "vsdg" .$image;
        return $comment ;
        return ""+$comment ;
        return $activity ;

    }
}
